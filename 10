#include <klee/klee.h>
#include <stdio.h>

// Some function never used:
void deadUtility() {
    printf("Dead Utility Called.\n");
}

int main() {
    int x;
    klee_make_symbolic(&x, sizeof(x), "x");

    if (x == 0) {
        // Return early, skipping the rest
        printf("x is zero, returning early.\n");
        return 0;
    }

    // If we get here, x != 0
    printf("x is not zero.\n");

    // Potentially dead or rarely used
    if (x == 99999) {
        printf("A large special value.\n");
    }

    return 0;
}
