#include <klee/klee.h>
#include <stdio.h>

int main() {
    int x, y;
    klee_make_symbolic(&x, sizeof(x), "x");
    klee_make_symbolic(&y, sizeof(y), "y");

    if (x > y) {
        printf("x is greater than y.\n");
    } else if (x < y) {
        printf("x is less than y.\n");
    } else {
        printf("x == y.\n");
    }

    // Dead path: only triggered if x+y == 999999, which is large and likely avoided
    if ((x + y) == 999999) {
        printf("Sum is 999999.\n");
    }

    return 0;
}
