#include <klee/klee.h>
#include <stdio.h>

int main() {
    int x, y;
    klee_make_symbolic(&x, sizeof(x), "x");
    klee_make_symbolic(&y, sizeof(y), "y");

    // Some arithmetic that doesn't affect the main output
    int unusedCalc = (x + y) * 2;
    // Do something minor
    if (x + y == 10) {
        printf("Sum is 10!\n");
    } else {
        printf("Sum is not 10.\n");
    }

    // Dead code not depending on anything
    if (unusedCalc == 123456) {
        printf("This is never printed under normal conditions.\n");
    }

    return 0;
}
