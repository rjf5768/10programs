#include <klee/klee.h>
#include <stdio.h>

int main() {
    int index;
    klee_make_symbolic(&index, sizeof(index), "index");

    int arr[5] = {1, 2, 3, 4, 5};

    // Basic boundary check
    if (index >= 0 && index < 5) {
        printf("arr[index] = %d\n", arr[index]);
    } else {
        printf("Index out of range.\n");
    }

    // Dead code
    if (index == 12345) {
        printf("You found a special index.\n");
    }

    return 0;
}
